{
    "name": "kolovious/meli-socialite",
    "description": "Mercadolibre Laravel Socialite Integration",
    "keywords": [
        "Laravel",
        "Socialite",
        "Mercadolibre"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Leonardo Garcia",
            "email": "cololeo@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "laravel/socialite": "4.*",
        "illuminate/support": "7.*",
        "ext-curl": "*"
    },
    "autoload": {
        "psr-4": {
            "Kolovious\\MeliSocialite\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kolovious\\MeliSocialite\\MeliSocialiteServiceProvider"
            ],
            "aliases": {
                "Meli": "Kolovious\\MeliSocialite\\Facade\\Meli"
            }
        }
    }
}
